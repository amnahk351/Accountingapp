    <script>

        function UpdatePopulateEntryModal(id) {

            $("#ModalJournalTable tbody tr").remove();

            var row = $(id).closest('tr');
            var table = document.getElementById("Events");

            var EntryModalHeader = document.getElementById("JournalEntryModalTitle");
            EntryModalHeader.innerText = "Updated Entry Details";

            var ID = table.rows[row[0].rowIndex].cells[0].innerText;
            var ModalID = document.getElementById("ModalEntryID");
            ModalID.innerHTML = "ID: " + ID;

            var Date = table.rows[row[0].rowIndex].cells[1].innerText;
            var ModalDate = document.getElementById("ModalDate");
            ModalDate.innerHTML = "Event Date: " + Date;


            var Query;

            $.ajax(
                {
                    url: '/Admin/RetrieveEditedDetailsTo/',
                    type: "GET",
                    async: false,
                    dataType: "json",
                    contentType: "application/json;",
                    data: { "id": ID },
                    success: function (result) {
                        //alert(result);
                        Query = result;

                    },
                    error: function (error) {
                        //alert(JSON.stringify(error));
                    }
                });

            var Query2 = Query.replace(/"/g, "");
            var Data = Query2.split("|^|");


            var EntryIDModal = document.getElementById("EntryID");
            EntryIDModal.innerHTML = "Journal Entry: " + Data[0];


            var ModalStatus = document.getElementById("ModalStatus");
            ModalStatus.innerHTML = Data[1];


            var ModalType = document.getElementById("ModalType");
            ModalType.innerHTML = Data[2];

            var JournalDetails = Data[3];

            var Items = JournalDetails.split(",");

            for (var i = 0; i < Items.length; i++) {

                var Split = Items[i].split("-");

                var tableRef = document.getElementById('ModalJournalTable').getElementsByTagName('tbody')[0];

                // Insert a row in the table at the last row
                var row = tableRef.insertRow(tableRef.rows.length);

                var cell1 = row.insertCell(0);
                var cell2 = row.insertCell(1);
                var cell3 = row.insertCell(2);
                cell1.innerHTML = Split[0];
                cell2.innerHTML = Split[1];
                cell3.innerHTML = Split[2];
            }

            var ModalComment = document.getElementById("ModalComment");
            ModalComment.innerHTML = Data[4];


        }


        function PopulateEntryModal(id) {

            $("#ModalJournalTable tbody tr").remove();

            var row = $(id).closest('tr');
            var table = document.getElementById("Events");

            var EntryModalHeader = document.getElementById("JournalEntryModalTitle");
            EntryModalHeader.innerText = "Old Entry Details";

            var ID = table.rows[row[0].rowIndex].cells[0].innerText;
            var ModalID = document.getElementById("ModalEntryID");
            ModalID.innerHTML = "ID: " + ID;

            var Date = table.rows[row[0].rowIndex].cells[1].innerText;
            var ModalDate = document.getElementById("ModalDate");
            ModalDate.innerHTML = "Event Date: " + Date;


            var Query;

            $.ajax(
                {
                    url: '/Admin/RetrieveEditedDetailsFrom/',
                    type: "GET",
                    async: false,
                    dataType: "json",
                    contentType: "application/json;",
                    data: { "id": ID },
                    success: function (result) {
                        //alert(result);
                        Query = result;

                    },
                    error: function (error) {
                        //alert(JSON.stringify(error));
                    }
                });

            var Query2 = Query.replace(/"/g, "");
            var Data = Query2.split("|^|");


            var EntryIDModal = document.getElementById("EntryID");
            EntryIDModal.innerHTML = "Journal Entry: " + Data[0];


            var ModalStatus = document.getElementById("ModalStatus");
            ModalStatus.innerHTML = Data[1];


            var ModalType = document.getElementById("ModalType");
            ModalType.innerHTML = Data[2];

            var JournalDetails = Data[3];

            var Items = JournalDetails.split(",");

            for (var i = 0; i < Items.length; i++) {

                var Split = Items[i].split("-");

                var tableRef = document.getElementById('ModalJournalTable').getElementsByTagName('tbody')[0];

                // Insert a row in the table at the last row
                var row = tableRef.insertRow(tableRef.rows.length);

                var cell1 = row.insertCell(0);
                var cell2 = row.insertCell(1);
                var cell3 = row.insertCell(2);
                cell1.innerHTML = Split[0];
                cell2.innerHTML = Split[1];
                cell3.innerHTML = Split[2];
            }

            var ModalComment = document.getElementById("ModalComment");
            ModalComment.innerHTML = Data[4];

        }

    </script>

