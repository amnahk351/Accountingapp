@model List<AccountingApp.Models.CreateUser>

@{
    ViewBag.Title = "Current Users";
}
<h2>@ViewBag.Title.</h2>



<div class="container">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>FirstName</th>
                <th>LastName</th>
                <th>Email</th>
                <th>Username</th>
                <th>Password</th>
                <th>Role</th>
                <th>Phone</th>
                <th>Date</th>
                <th>Action</th>

            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.Count(); i++)
            {
                <tr>
                    <td>@Model[i].ID</td>
                    <td>@Model[i].FirstName</td>
                    <td>@Model[i].LastName</td>
                    <td>@Model[i].Email</td>
                    <td>@Model[i].Username</td>
                    <td>@Model[i].Password</td>
                    <td>@Model[i].Role</td>
                    <td>@Model[i].Phone</td>
                    <td>@Model[i].Date</td>

                    <td>
                        <a href="@Url.Action("Delete", new { id = Model[i].ID})"><i class="glyphicon glyphicon-off"></i></a>
                        <a href="@Url.Action("Edit", new { id = Model[i].ID})"><i class="glyphicon glyphicon-edit"></i></a>

                    </td>

                </tr>
            }
        </tbody>
    </table>

</div>